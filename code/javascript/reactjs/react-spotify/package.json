{
  "name": "react-spotify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.25.1",
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.1.1",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "code:style": "find src/ -name \"*.js\" | xargs yarn-or-npm prettier --write",
    "lint": "concurrently \"yarn-or-npm lint:js\" \"yarn-or-npm lint:css\"",
    "lint:js": "eslint ./src",
    "lint:css": "cross-env stylelint ./src",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --runInBand --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-plugin-import": "^1.12.2",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-config-wiremore": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-styled-components": "^6.3.4",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "react-dev-utils": "^9.1.0",
    "react-test-renderer": "^16.12.0",
    "redux-devtools": "^3.5.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "yarn-or-npm": "^3.0.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/config/*.{js,jsx,ts,tsx}",
      "!src/routes/*.{js,jsx,ts,tsx}",
      "!src/store/*.{js,jsx,ts,tsx}",
      "!src/**/*-wrappers.{js,jsx,ts,tsx}",
      "!src/**/*-wrapper.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!<rootDir>/src/screens/**",
      "!<rootDir>/src/components/icons/*",
      "!<rootDir>/src/App.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/serviceWorker.js"
    ]
  }
}
